  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec ts-node --esm src/server.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "seed": "nodemon --exec ts-node --esm src/database/seedDatabase.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -d src/database/ormconfig.ts -n Migration1 src/database/migrations/Migration1.ts",
    "migration:run": "npx typeorm migration:run -d src/database/ormconfig.ts",
    "migration:revert": "npx typeorm migration:revert -d src/database/ormconfig.ts"
  },
